		"kdyby/annotations": "^2.4",
		"nette/application": "^2.4",
		"nette/bootstrap": "^2.4",
		"nette/caching": "^2.4",
		"nette/component-model": "^2.2",
		"nette/database": "^2.4",
		"nette/deprecated": "^2.4",
		"nette/finder": "^2.4",
		"nette/http": "^2.4",
		"nette/mail": "^2.4",
