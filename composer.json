	"keywords": ["nette", "kdyby", "doctrine", "annotations", "mapper"],
	"license": ["BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
	"authors": [
		{
			"name": "Ondrej Pecina",
			"email": "pecina.ondrej@gmail.com"
		}
	],
	"support": {
		"email": "filip@prochazka.su",
		"issues": "https://github.com/kdyby/annotations/issues"
	},
	"require": {
		"nette/utils": "^2.4",
		"nette/forms": "^2.4",
		"nette/di": "^2.4",
		"nette/reflection": "^2.4",
		"kdyby/annotations": "^2.4",
		"kdyby/doctrine": "^3.1.1"
	},
	"require-dev": {
		"nette/utils": "^2.4",
		"nette/forms": "^2.4",
		"nette/di": "^2.4",
