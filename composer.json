		"nette/php-generator": "^2.4",
		"nette/robot-loader": "^2.4",
		"nette/security": "^2.4",
		"nette/tokenizer": "~2.2@dev",
		"latte/latte": "^2.4",
		"tracy/tracy": "^2.4",
		"nette/tester": "~1.3@rc"
	},
	"conflict": {
		"nette/nette": "<2.2"
